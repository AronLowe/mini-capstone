<h1>Create new listing</h1>

<%= form_tag "/listings", method: :post do %>

<div>
Name: <input type="text" name="form_name">  
</div>
<div>
Price: <input type="text" name="form_price">  
</div>
<div>
Image: <input type="text" name="form_image">  
</div>
<div>
Description: <input type="text" name="form_description">  
</div>
<input type="submit" value="Create Listing">

<% end %>